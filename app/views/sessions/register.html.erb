<h1> Register </h1>
<%= form_for :user, url: "/register" do |f| %>
    
    <%= f.label :username %>
    <%= f.text_field :username %> <br></br>

    <%= f.label :email %>
    <%= f.email_field :email %> <br></br>

    <%= f.label :password %>
    <%= f.password_field :password %> <br></br>

    <%= f.submit "Register" %>
<% end %>
<br>
<%= form_tag("/auth/google_oauth2") do %>
    <%= submit_tag "Google Login"%>
<% end %>